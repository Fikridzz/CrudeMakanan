<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fikridzakwan.crudemakanan">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".UI.main.MainActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".UI.login.LoginActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UI.register.RegisterActivity" />
        <activity android:name=".UI.uploadmakanan.UploadMakananActivity" />
        <activity android:name=".UI.detailmakanan.DetailMakananActivity" />
        <activity android:name=".UI.MakananByKategori.MakananByKategoriActivity" />
        <activity android:name=".UI.DetailMakanan.DetailMakananActivity" />
        <activity android:name=".UI.detailbyuser.DetailByUserActivity"></activity>
    </application>

</manifest>